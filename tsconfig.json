{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"baseUrl": "./src",
		"allowJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true,
    	"jsx": "react",
		"paths": {
			"app": ["src/app"]
		}
	},
}
